<Window x:Class="Voxura.WpfDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Voxura.WpfDemo"
        xmlns:views="clr-namespace:Voxura.WpfDemo.Views"
        xmlns:vm ="clr-namespace:Voxura.WpfDemo.ViewModels"
        mc:Ignorable="d"
        d:DataContext = "{d:DesignInstance vm:MainViewModel, IsDesignTimeCreatable=True}"
        Title="MainWindow" Height="600" Width="800">
    <StackPanel>
        <views:RFQForm DataContext="{Binding RFQForm}" />
        <TextBox Name="UserText" Text="{Binding UserText, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="20"/>
    </StackPanel>
</Window>
